<div class="greeting"><h3>Hello, <%= @user.email %></h3></div>

<div class="user-page-container">
  <div class="list-gifts-container">
      <% @bookmarks.each do |bookmark| %>
      <table width="800">
        <tbody>
        <tr>
          <td><h3><%= bookmark.title %></h3></td>
          <td><a href="<%= bookmark.url %>" target="_blank"><%= bookmark.url %></a></td>
        </tr>
        </tbody>
      </table>
      <% end %>
</div>